// C implementation to demonstrate
   // the usage of arrays
 
#include <stdio.h>
 
int main()
{
    // Array Indexs-0   1   2   3   4
    int arr[5] = { 10, 20, 30, 40, 50 };
 
    int size = sizeof(arr);
 
    printf("Size of array is %d\n", size);
 
    printf("Data at index 2 is %d\n", arr[2]);
}
